; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CivLike"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Globals="*res://autoloads/globals.gd"
Events="*res://autoloads/events.gd"
TimeTracker="*res://autoloads/time_tracker.gd"

[file_customization]

folder_colors={
"res://assets/": "red",
"res://autoloads/": "pink",
"res://resources/": "yellow",
"res://scenes/": "orange",
"res://scripts/": "green"
}

[global_group]

Item="Item"
Base="Base"
MobBody="MobBody"

[layer_names]

2d_physics/layer_1="Mobs"
2d_physics/layer_2="Objects"
2d_physics/layer_3="Projectiles"
2d_physics/layer_4="Gui"
2d_physics/layer_5="Wall"
2d_physics/layer_6="Floor"
2d_physics/layer_7="Storage"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
